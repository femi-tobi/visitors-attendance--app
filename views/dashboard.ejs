<!DOCTYPE html>
<html>
<head>
  <title>Admin Dashboard</title>
  <link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
</head>
<body class="bg-gray-100 p-6">
  <h1 class="text-2xl font-bold mb-4">Visitor Logs</h1>
  <table class="min-w-full bg-white">
    <thead>
      <tr>
        <th class="px-4 py-2">Name</th>
        <th class="px-4 py-2">Email</th>
        <th class="px-4 py-2">Staff</th>
        <th class="px-4 py-2">Reason</th>
        <th class="px-4 py-2">Status</th>
        <th class="px-4 py-2">Time</th>
      </tr>
    </thead>
    <tbody>
      <% visits.forEach(v => { %>
      <tr class="text-center">
        <td class="border px-4 py-2"><%= v.full_name %></td>
        <td class="border px-4 py-2"><%= v.email %></td>
        <td class="border px-4 py-2"><%= v.staff_email %></td>
        <td class="border px-4 py-2"><%= v.reason %></td>
        <td class="border px-4 py-2"><%= v.status %></td>
        <td class="border px-4 py-2"><%= v.visit_time %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
